{
  "name": "event-api",
  "private": true,
  "version": "1.0.0",
  "description": "a code challenge for building an event API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --listEmittedFiles",
    "clean": "npx rimraf \"{node_modules,dist}\"",
    "dev": "concurrently --handle-input --names \"build,api\" -c \"bgBlue.bold,bgMagenta.bold\" \"tsc --listEmittedFiles --watch\" \"nodemon\"",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yadam/event-api.git"
  },
  "author": "Adam Bantly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yadam/event-api/issues"
  },
  "homepage": "https://github.com/yadam/event-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.1",
    "@types/memory-cache": "^0.2.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.19.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "cross-env": "^7.0.0",
    "cuid": "^2.1.8",
    "express": "^4.17.3"
  }
